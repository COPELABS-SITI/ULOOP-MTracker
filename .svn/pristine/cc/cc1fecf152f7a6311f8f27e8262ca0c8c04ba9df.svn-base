        <tr>
          <td class="memname">void eu.uloop.mobilitytracker.MTrackerService.announcePossibleHandover </td>
          <td>(</td>
          <td class="paramtype">String&#160;</td>
          <td class="paramname"><em>nextBssid</em>, </td>
        </tr>
        <tr>
          <td class="paramkey"></td>
